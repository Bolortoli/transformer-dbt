# dbt Environment Configuration Example
# Copy this file to .env and set your actual values

# Target environment (dev, staging, prod)
DBT_TARGET=dev

# BigQuery Configuration
BIGQUERY_PROJECT_ID=your-gcp-project-id
BIGQUERY_LOCATION=US

# Environment-specific datasets
# Development
BIGQUERY_DATASET=transformer_dbt_dev

# Staging (uncomment for staging deployment)
# BIGQUERY_DATASET=transformer_dbt_staging

# Production (uncomment for production deployment)  
# BIGQUERY_DATASET=transformer_dbt_prod

# Authentication
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json

# Optional: Custom configurations
# DBT_PROFILES_DIR=/path/to/profiles/directory
# DBT_LOG_LEVEL=info